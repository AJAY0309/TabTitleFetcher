{
    "manifest_version": 3,
    "name": "Tab Title Fetcher",
    "version": "1.0",
    "description": "Fetches and displays the title of the current tab.",
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": "/images/icon.png"
    },
    "icons": {
        "128": "/images/icon_128.png"
    }
}